{
	"name": "sysml",
	"displayName": "SysML",
	"description": "SysML support for VSCode",
	"version": "0.1.2",
	"publisher": "martinring",
	"repository": {
		"url": "https://github.com/martinring/vscode-sysml"
	},
	"engines": {
		"vscode": "^0.10.6"
	},
	"categories": [
		"Languages"
	],
